{
  "openapi": "3.0.3",
  "info": {
    "description": "My Football App",
    "title": "My Football App",
    "version": "1.0.0"
  },
  "servers": [
    {
      "url": "https://api-football-beta.p.rapidapi.com",
      "description": "Beta"
    }
  ],
  "tags": [],
  "paths": {
    "/leagues": {
      "get": {
        "security": [
          {
            "jwt": []
          }
        ],
        "description": "Get list of Leagues",
        "tags": ["Leagues"],
        "parameters": [
          {
            "in": "query",
            "name": "name",
            "required": false,
            "description": "Get list of Leagues by name",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/Leagues"
                  }
                }
              }
            }
          }
        }
      }
    },
    "/teams": {
      "get": {
        "security": [
          {
            "jwt": []
          }
        ],
        "description": "Get list of Teams",
        "tags": ["Leagues"],
        "parameters": [
          {
            "in": "query",
            "name": "name",
            "required": false,
            "description": "Get list of Teams by name",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/Teams"
                  }
                }
              }
            }
          }
        }
      }
    },
    "/standings": {
      "get": {
        "security": [
          {
            "jwt": []
          }
        ],
        "description": "Get list of standings",
        "tags": ["Leagues"],
        "parameters": [
          {
            "in": "query",
            "name": "id",
            "required": false,
            "description": "Get list of standings by season and team id  or league id",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/Standings"
                  }
                }
              }
            }
          }
        }
      }
    },
    "/fixtures": {
      "get": {
        "security": [
          {
            "jwt": []
          }
        ],
        "description": "Get list of standings",
        "tags": ["Leagues"],
        "parameters": [
          {
            "in": "query",
            "name": "id",
            "required": true,
            "description": "Get list of Fixtures by season and team id",
            "schema": {
              "type": "number"
            }
          },
          {
            "in": "query",
            "name": "season",
            "required": true,
            "description": "Get list of Fixtures by season and team id",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/Fixtures"
                  }
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "Leagues": {
        "type": "object",
        "properties": {
          "league": {
            "$ref": "#/components/schemas/League"
          },
          "country": {
            "$ref": "#/components/schemas/Country"
          },
          "seasons": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/Season"
            }
          }
        }
      },
      "League": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "readOnly": true
          },
          "name": {
            "type": "string"
          },
          "logo": {
            "type": "string"
          },
          "flag": {
            "type": "string"
          },
          "country": {
            "type": "string"
          },
          "type": {
            "type": "string"
          }
        },
        "required": ["name"]
      },
      "Country": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string"
          },
          "code": {
            "type": "string"
          },
          "flag": {
            "type": "string"
          }
        },
        "required": ["name"]
      },
      "Season": {
        "type": "object",
        "properties": {
          "year": {
            "type": "string"
          },
          "start": {
            "type": "string"
          },
          "end": {
            "type": "string"
          },
          "coverage": {
            "$ref": "#/components/schemas/Coverage"
          }
        }
      },
      "SeasonFixture": {
        "type": "object",
        "properties": {
          "events": {
            "type": "boolean"
          },
          "lineups": {
            "type": "boolean"
          },
          "statistics_fixtures": {
            "type": "boolean"
          },
          "statistics_players": {
            "type": "boolean"
          }
        }
      },
      "Coverage": {
        "type": "object",
        "properties": {
          "standings": {
            "type": "boolean"
          },
          "players": {
            "type": "boolean"
          },
          "top_scorers": {
            "type": "boolean"
          },
          "top_assists": {
            "type": "boolean"
          },
          "top_cards": {
            "type": "boolean"
          },
          "injuries": {
            "type": "boolean"
          },
          "predictions": {
            "type": "boolean"
          },
          "odds": {
            "type": "boolean"
          },
          "fixtures": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/SeasonFixture"
            }
          }
        }
      },
      "Teams": {
        "type": "object",
        "properties": {
          "team": {
            "$ref": "#/components/schemas/Team"
          },
          "venue": {
            "$ref": "#/components/schemas/Venue"
          }
        }
      },
      "Team": {
        "type": "object",
        "properties": {
          "id": {
            "type": "number"
          },
          "name": {
            "type": "string"
          },
          "country": {
            "type": "string"
          },
          "founded": {
            "type": "number"
          },
          "national": {
            "type": "boolean"
          },
          "logo": {
            "type": "string"
          },
          "winner": {
            "type": "boolean"
          }
        }
      },
      "Venue": {
        "type": "object",
        "properties": {
          "id": {
            "type": "number"
          },
          "name": {
            "type": "string"
          },
          "address": {
            "type": "string"
          },
          "city": {
            "type": "number"
          },
          "capacity": {
            "type": "number"
          },
          "surface": {
            "type": "string"
          },
          "image": {
            "type": "string"
          }
        }
      },
      "Periods": {
        "type": "object",
        "properties": {
          "first": {
            "type": "number"
          },
          "second": {
            "type": "number"
          }
        }
      },
      "Fixture": {
        "type": "object",
        "properties": {
          "id": {
            "type": "number"
          },
          "referee": {
            "type": "string"
          },
          "timezone": {
            "type": "string"
          },
          "date": {
            "type": "string"
          },
          "timestamp": {
            "type": "number"
          },
          "periods": {
            "$ref": "#/components/schemas/Periods"
          },
          "venue": {
            "$ref": "#/components/schemas/Venue"
          },
          "status": {
            "type": "object",
            "properties": {
              "long": {
                "type": "string"
              },
              "short": {
                "type": "string"
              },
              "elapsed": {
                "type": "number"
              }
            }
          }
        }
      },
      "FixtureTeams": {
        "type": "object",
        "properties": {
          "home": {
            "$ref": "#/components/schemas/Team"
          },
          "away": {
            "$ref": "#/components/schemas/Team"
          }
        }
      },
      "FixtureGoals": {
        "type": "object",
        "properties": {
          "home": {
            "type": "number"
          },
          "away": {
            "type": "number"
          }
        }
      },
      "FixtureScores": {
        "type": "object",
        "properties": {
          "halftime": {
            "$ref": "#/components/schemas/FixtureGoals"
          },
          "fulltime": {
            "$ref": "#/components/schemas/FixtureGoals"
          },
          "extratime": {
            "$ref": "#/components/schemas/FixtureGoals"
          },
          "penalty": {
            "$ref": "#/components/schemas/FixtureGoals"
          }
        }
      },
      "FixtureLeague": {
        "allOf": [
          {
            "$ref": "#/components/schemas/League"
          },
          {
            "type": "object",
            "required": ["standings"],
            "properties": {
              "season": {
                "type": "string"
              },
              "round": {
                "type": "string"
              }
            }
          }
        ]
      },
      "Fixtures": {
        "type": "object",
        "properties": {
          "fixture": {
            "$ref": "#/components/schemas/Fixture"
          },
          "league": {
            "$ref": "#/components/schemas/FixtureLeague"
          },
          "teams": {
            "$ref": "#/components/schemas/FixtureTeams"
          },
          "goals": {
            "$ref": "#/components/schemas/FixtureGoals"
          },
          "scores": {
            "$ref": "#/components/schemas/FixtureScores"
          }
        }
      },
      "StandingGoals": {
        "type": "object",
        "properties": {
          "for": {
            "type": "number"
          },
          "againts": {
            "type": "number"
          }
        }
      },
      "StandingGame": {
        "type": "object",
        "properties": {
          "played": {
            "type": "number"
          },
          "win": {
            "type": "number"
          },
          "draw": {
            "type": "number"
          },
          "lose": {
            "type": "number"
          },
          "goals": {
            "$ref": "#/components/schemas/StandingGoals"
          }
        }
      },
      "StandingTeam": {
        "type": "object",
        "properties": {
          "rank": {
            "type": "number"
          },
          "team": {
            "$ref": "#/components/schemas/Team"
          },
          "points": {
            "type": "number"
          },
          "goalsDiff": {
            "type": "number"
          },
          "group": {
            "type": "string"
          },
          "form": {
            "type": "string"
          },
          "status": {
            "type": "string"
          },
          "description": {
            "type": "string"
          },
          "all": {
            "$ref": "#/components/schemas/StandingGame"
          },
          "home": {
            "$ref": "#/components/schemas/StandingGame"
          },
          "away": {
            "$ref": "#/components/schemas/StandingGame"
          }
        }
      },
      "StandingArray": {
        "type": "array",
        "items": {
          "$ref": "#/components/schemas/StandingTeam"
        }
      },
      "StandingLeague": {
        "allOf": [
          {
            "$ref": "#/components/schemas/League"
          },
          {
            "type": "object",
            "required": ["standings"],
            "properties": {
              "season": {
                "type": "string"
              },
              "standings": {
                "type": "array",
                "items": {
                  "$ref": "#/components/schemas/StandingArray"
                }
              }
            }
          }
        ]
      },
      "Standings": {
        "type": "object",
        "properties": {
          "league": {
            "$ref": "#/components/schemas/StandingLeague"
          }
        }
      }
    },
    "securitySchemes": {
      "jwt": {
        "type": "http",
        "scheme": "bearer",
        "bearerFormat": "JWT",
        "description": "See #components/schemas/Jwt"
      }
    }
  },
  "security": []
}
